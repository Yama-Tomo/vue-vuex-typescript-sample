{
  "name": "vue-sample",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development vue-cli-service serve --port 3100 src/index.ts",
    "build": "NODE_ENV=production vue-cli-service build --mode production",
    "build:dev": "NODE_ENV=development vue-cli-service build --mode development",
    "lint": "eslint --ext .ts,.tsx,.js,.vue --format codeframe --color",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn lint --fix"
    ]
  },
  "dependencies": {
    "vue": "2.6.14",
    "vuex": "3.6.2",
    "vuex-type-helper": "1.3.1"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config-typescript": "6.0.1",
    "@types/lodash": "4.14.171",
    "@typescript-eslint/eslint-plugin": "4.28.5",
    "@typescript-eslint/parser": "4.28.5",
    "@vue/cli-plugin-babel": "4.5.13",
    "@vue/cli-plugin-typescript": "4.5.13",
    "@vue/cli-service": "4.5.13",
    "eslint": "7.29.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-prettier": "3.4.0",
    "husky": "6.0.0",
    "javascript-typescript-langserver": "2.11.3",
    "lint-staged": "11.0.1",
    "node-sass": "6.0.1",
    "sass-loader": "10.2.0",
    "typescript": "4.3.5",
    "vue-template-compiler": "2.6.14",
    "vue-tsx-support": "3.2.0",
    "webpack-manifest-plugin": "3.1.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
