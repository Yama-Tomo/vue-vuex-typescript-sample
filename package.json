{
  "name": "vue-sample",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .ts,.tsx,.js,.vue --format codeframe --color",
    "dev": "nuxt",
    "test": "NUXT_PORT=4444 jest",
    "build": "nuxt build && tsc -p tsconfig.nuxt-config.json",
    "start": "./server.js start -c .nuxt/ts-compiled/nuxt.config.js",
    "start:with_ts_node": "nuxt start",
    "lint-staged": "lint-staged",
    "coverage": "codecov"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn lint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@nuxtjs/auth": "github:Yama-Tomo/auth-module#feature/allow_redirect_setting_by_function",
    "@nuxtjs/axios": "5.4.1",
    "body-parser": "1.19.0",
    "lodash": "4.17.11",
    "nuxt": "2.7.1",
    "nuxt-i18n": "5.12.3",
    "nuxt-property-decorator": "2.1.3",
    "vuex-type-helper": "1.2.2"
  },
  "devDependencies": {
    "@nuxt/typescript": "2.7.1",
    "@nuxtjs/eslint-config": "0.0.1",
    "@types/body-parser": "1.17.0",
    "@types/expect-puppeteer": "3.3.1",
    "@types/hard-source-webpack-plugin": "1.0.1",
    "@types/jest": "24.0.13",
    "@types/jest-environment-puppeteer": "4.0.0",
    "@types/lodash": "4.14.129",
    "@types/node": "11.13.10",
    "@types/puppeteer": "1.12.4",
    "@types/webpack": "4.4.31",
    "@typescript-eslint/eslint-plugin": "1.9.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "codecov": "3.5.0",
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.3.0",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-import": "2.17.2",
    "eslint-plugin-jest": "22.5.1",
    "eslint-plugin-node": "9.0.1",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-promise": "4.1.1",
    "eslint-plugin-standard": "4.0.0",
    "eslint-plugin-vue": "5.2.2",
    "hard-source-webpack-plugin": "0.13.1",
    "husky": "2.3.0",
    "jest": "24.8.0",
    "jest-puppeteer": "4.1.1",
    "jest-serializer-vue": "^2.0.2",
    "json-loader": "0.5.7",
    "lint-staged": "8.1.7",
    "node-sass": "4.12.0",
    "puppeteer": "1.16.0",
    "sass-loader": "7.1.0",
    "ts-jest": "24.0.2",
    "ts-node": "8.1.0",
    "typescript": "3.4.5",
    "vue-jest": "^3.0.4",
    "vue-tsx-support": "2.3.0",
    "yaml-loader": "0.5.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
