{
  "name": "vue-sample",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development vue-cli-service serve --port 3100 src/index.ts",
    "build": "NODE_ENV=production vue-cli-service build --mode production",
    "build:dev": "NODE_ENV=development vue-cli-service build --mode development",
    "lint": "eslint --ext .ts,.tsx,.js,.vue --format codeframe --color",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn lint --fix"
    ]
  },
  "dependencies": {
    "vue": "2.6.14",
    "vuex": "3.6.2",
    "vuex-type-helper": "1.3.1"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config-typescript": "7.0.2",
    "@types/lodash": "4.17.12",
    "@typescript-eslint/eslint-plugin": "5.8.0",
    "@typescript-eslint/parser": "5.8.0",
    "@vue/cli-plugin-babel": "4.5.15",
    "@vue/cli-plugin-typescript": "4.5.15",
    "@vue/cli-service": "4.5.15",
    "eslint": "8.5.0",
    "eslint-config-prettier": "8.10.0",
    "eslint-formatter-codeframe": "7.32.1",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "7.0.4",
    "javascript-typescript-langserver": "2.11.3",
    "lint-staged": "12.5.0",
    "node-sass": "6.0.1",
    "prettier": "2.8.8",
    "sass-loader": "10.5.2",
    "typescript": "4.9.5",
    "vue-template-compiler": "2.6.14",
    "vue-tsx-support": "3.2.0",
    "webpack-manifest-plugin": "4.1.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
