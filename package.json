{
  "name": "vue-sample",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development vue-cli-service serve --port 3100 src/index.ts",
    "build": "NODE_ENV=production vue-cli-service build --mode production",
    "build:dev": "NODE_ENV=development vue-cli-service build --mode development",
    "lint": "eslint --ext .ts,.js,.vue --format codeframe --color .",
    "lint:commit": ".scripts/lint_when_commit"
  },
  "pre-commit": [
    "lint:commit"
  ],
  "dependencies": {
    "vue": "2.6.10",
    "vue-property-decorator": "8.1.0",
    "vuex": "3.1.0",
    "vuex-type-helper": "1.2.2"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "0.0.1",
    "@typescript-eslint/eslint-plugin": "1.6.0",
    "@vue/cli-plugin-babel": "3.5.5",
    "@vue/cli-plugin-typescript": "3.5.3",
    "@vue/cli-service": "3.5.3",
    "eslint": "5.16.0",
    "eslint-config-standard": "12.0.0",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-import": "2.17.0",
    "eslint-plugin-jest": "22.4.1",
    "eslint-plugin-node": "8.0.1",
    "eslint-plugin-promise": "4.1.1",
    "eslint-plugin-standard": "4.0.0",
    "eslint-plugin-vue": "5.2.2",
    "javascript-typescript-langserver": "2.11.3",
    "node-sass": "4.11.0",
    "pre-commit": "1.2.2",
    "sass-loader": "7.1.0",
    "typescript": "3.4.3",
    "vue-template-compiler": "2.6.10",
    "vue-tsx-support": "2.2.2",
    "webpack-manifest-plugin": "2.0.4"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
