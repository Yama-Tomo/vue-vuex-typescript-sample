{
  "name": "vue-sample",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development vue-cli-service serve --port 3100 src/index.ts",
    "build": "NODE_ENV=production vue-cli-service build --mode production",
    "build:dev": "NODE_ENV=development vue-cli-service build --mode development",
    "lint": "eslint --ext .ts,.tsx,.js,.vue --format codeframe --color",
    "lint-staged": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn lint --fix"
    ]
  },
  "dependencies": {
    "vue": "2.6.12",
    "vuex": "3.5.1",
    "vuex-type-helper": "1.2.2"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config-typescript": "4.0.0",
    "@types/lodash": "4.14.162",
    "@typescript-eslint/eslint-plugin": "4.6.0",
    "@typescript-eslint/parser": "4.6.0",
    "@vue/cli-plugin-babel": "4.5.8",
    "@vue/cli-plugin-typescript": "4.5.8",
    "@vue/cli-service": "4.5.8",
    "eslint": "7.12.0",
    "eslint-config-prettier": "6.14.0",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-prettier": "3.1.4",
    "husky": "4.3.0",
    "javascript-typescript-langserver": "2.11.3",
    "lint-staged": "10.5.0",
    "node-sass": "4.14.1",
    "sass-loader": "10.0.4",
    "typescript": "4.0.5",
    "vue-template-compiler": "2.6.12",
    "vue-tsx-support": "3.0.2",
    "webpack-manifest-plugin": "2.2.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
