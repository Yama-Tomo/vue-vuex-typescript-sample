{
  "name": "vue-sample",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development vue-cli-service serve --port 3100 src/index.ts",
    "build": "NODE_ENV=production vue-cli-service build --mode production",
    "build:dev": "NODE_ENV=development vue-cli-service build --mode development",
    "lint": "eslint --ext .ts,.tsx,.js,.vue --format codeframe --color",
    "lint-staged": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn lint --fix"
    ]
  },
  "dependencies": {
    "vue": "2.6.11",
    "vue-property-decorator": "8.4.1",
    "vuex": "3.1.3",
    "vuex-type-helper": "1.2.2"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config-typescript": "1.0.2",
    "@types/lodash": "4.14.149",
    "@typescript-eslint/eslint-plugin": "2.25.0",
    "@typescript-eslint/parser": "2.25.0",
    "@vue/cli-plugin-babel": "4.2.3",
    "@vue/cli-plugin-typescript": "4.2.3",
    "@vue/cli-service": "4.2.3",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-prettier": "3.1.2",
    "husky": "4.2.3",
    "javascript-typescript-langserver": "2.11.3",
    "lint-staged": "10.0.10",
    "node-sass": "4.13.1",
    "sass-loader": "8.0.2",
    "typescript": "3.8.3",
    "vue-template-compiler": "2.6.11",
    "vue-tsx-support": "2.3.3",
    "webpack-manifest-plugin": "2.2.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
