{
  "name": "vue-sample",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development vue-cli-service serve --port 3100 src/index.ts",
    "build": "NODE_ENV=production vue-cli-service build --mode production",
    "build:dev": "NODE_ENV=development vue-cli-service build --mode development",
    "lint": "eslint --ext .ts,.tsx,.js,.vue --format codeframe --color",
    "lint-staged": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn lint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "vue": "2.6.10",
    "vue-property-decorator": "8.2.2",
    "vuex": "3.1.2",
    "vuex-type-helper": "1.2.2"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config-typescript": "0.1.3",
    "@types/lodash": "4.14.149",
    "@typescript-eslint/eslint-plugin": "2.3.3",
    "@typescript-eslint/parser": "2.3.3",
    "@vue/cli-plugin-babel": "3.11.0",
    "@vue/cli-plugin-typescript": "3.11.0",
    "@vue/cli-service": "3.11.0",
    "eslint": "6.5.1",
    "eslint-config-prettier": "6.3.0",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-prettier": "3.1.1",
    "husky": "3.0.9",
    "javascript-typescript-langserver": "2.11.3",
    "lint-staged": "9.4.3",
    "node-sass": "4.12.0",
    "sass-loader": "8.0.0",
    "typescript": "3.6.4",
    "vue-template-compiler": "2.6.10",
    "vue-tsx-support": "2.3.2",
    "webpack-manifest-plugin": "2.1.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
