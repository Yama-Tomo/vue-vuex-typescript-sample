{
  "name": "vue-sample",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .ts,.tsx,.js,.vue --format codeframe --color",
    "dev": "nuxt-ts",
    "test": "jest",
    "build": "nuxt-ts build && tsc -p tsconfig.nuxt-config.json",
    "start": "nuxt start -c .nuxt/ts-compiled/nuxt.config.js",
    "start:with_ts_node": "nuxt-ts start",
    "lint-staged": "lint-staged",
    "coverage": "codecov",
    "sb": "start-storybook -p 6006",
    "sb:build": "build-storybook",
    "postinstall": ".npm_scripts/uninstall_specific_packages.sh"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": [
      "yarn lint --fix"
    ]
  },
  "dependencies": {
    "@nuxtjs/auth": "4.9.1",
    "body-parser": "1.19.0",
    "lodash": "4.17.15",
    "nuxt": "2.12.2",
    "nuxt-i18n": "6.9.1",
    "nuxt-property-decorator": "2.5.1",
    "vue": "2.6.11",
    "vuex-type-helper": "1.2.2"
  },
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@mdi/js": "5.1.45",
    "@nuxt/typescript-build": "0.6.5",
    "@nuxt/typescript-runtime": "0.4.5",
    "@nuxtjs/eslint-config-typescript": "1.0.2",
    "@nuxtjs/vuetify": "2.0.0-beta.0",
    "@storybook/addon-storyshots": "5.3.18",
    "@storybook/addon-storyshots-puppeteer": "5.3.18",
    "@storybook/vue": "5.3.18",
    "@types/body-parser": "1.19.0",
    "@types/expect-puppeteer": "4.4.1",
    "@types/hard-source-webpack-plugin": "1.0.1",
    "@types/jest-environment-puppeteer": "4.3.1",
    "@types/lodash": "4.14.150",
    "@types/puppeteer": "2.0.1",
    "@vue/test-utils": "1.0.0-beta.33",
    "babel-core": "7.0.0-bridge.0",
    "babel-plugin-require-context-hook": "1.0.0",
    "babel-plugin-rewire": "1.2.0",
    "babel-preset-vue": "2.0.2",
    "codecov": "3.6.5",
    "core-js": "2.6.11",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-prettier": "3.1.3",
    "husky": "4.2.5",
    "jest": "25.4.0",
    "jest-css-modules": "2.1.0",
    "jest-junit": "10.0.0",
    "jest-puppeteer": "4.4.0",
    "jest-serializer-vue": "2.0.2",
    "jest-yaml-transform": "0.2.0",
    "json-loader": "0.5.7",
    "lint-staged": "10.1.7",
    "prettier": "2.0.5",
    "puppeteer": "2.1.1",
    "ts-jest": "25.4.0",
    "typescript": "3.8.3",
    "vue-fragment": "1.5.1",
    "vue-jest": "3.0.5",
    "vue-tsx-support": "2.3.3",
    "vuetify-jsx-loader": "0.1.2",
    "yaml-loader": "0.6.0"
  },
  "resolutions": {
    "vue-style-loader": "https://github.com/Yama-Tomo/vue-style-loader/archive/master.tar.gz"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
