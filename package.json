{
  "name": "vue-sample",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .ts,.tsx,.js,.vue --format codeframe --color",
    "dev": "nuxt",
    "test": "NUXT_PORT=4444 jest",
    "test:use-docker": "docker-compose -f docker-compose-circleci.yml -f docker-compose-local.yml run --rm puppeteer",
    "build": "nuxt build && tsc -p tsconfig.nuxt-config.json",
    "start": "./server.js start -c .nuxt/ts-compiled/nuxt.config.js",
    "start:with_ts_node": "nuxt start",
    "lint-staged": "lint-staged",
    "coverage": "codecov",
    "sb": "start-storybook -p 6006",
    "sb:build": "build-storybook",
    "postinstall": ".npm_scripts/uninstall_specific_packages.sh"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": [
      "yarn lint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@nuxtjs/auth": "4.8.1",
    "@nuxtjs/axios": "5.5.4",
    "body-parser": "1.19.0",
    "lodash": "4.17.15",
    "nuxt": "2.8.1",
    "nuxt-i18n": "6.0.0",
    "nuxt-property-decorator": "2.3.0",
    "vue": "2.6.10",
    "vuex-type-helper": "1.2.2"
  },
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@babel/preset-env": "7.5.5",
    "@babel/runtime-corejs2": "7.5.5",
    "@nuxt/typescript": "2.8.1",
    "@nuxtjs/eslint-config": "1.0.1",
    "@storybook/addon-storyshots": "5.1.10",
    "@storybook/addon-storyshots-puppeteer": "5.1.10",
    "@storybook/vue": "5.1.10",
    "@types/body-parser": "1.17.0",
    "@types/expect-puppeteer": "3.3.1",
    "@types/hard-source-webpack-plugin": "1.0.1",
    "@types/jest": "24.0.16",
    "@types/jest-environment-puppeteer": "4.0.0",
    "@types/lodash": "4.14.136",
    "@types/node": "12.6.8",
    "@types/puppeteer": "1.19.0",
    "@types/storybook__addon-storyshots": "4.0.2",
    "@types/storybook__vue": "5.0.2",
    "@types/webpack": "4.32.1",
    "@typescript-eslint/eslint-plugin": "1.13.0",
    "@typescript-eslint/parser": "1.13.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-loader": "8.0.6",
    "babel-plugin-require-context-hook": "1.0.0",
    "babel-plugin-rewire": "1.2.0",
    "babel-preset-vue": "2.0.2",
    "codecov": "3.5.0",
    "core-js": "2.6.9",
    "css-loader": "2.1.1",
    "eslint": "6.1.0",
    "eslint-config-prettier": "6.0.0",
    "eslint-config-standard": "13.0.1",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jest": "22.14.1",
    "eslint-plugin-node": "9.1.0",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.0",
    "eslint-plugin-vue": "5.2.3",
    "hard-source-webpack-plugin": "0.13.1",
    "husky": "3.0.2",
    "jest": "24.8.0",
    "jest-junit": "7.0.0",
    "jest-puppeteer": "4.3.0",
    "jest-serializer-vue": "2.0.2",
    "jest-yaml-transform": "0.2.0",
    "json-loader": "0.5.7",
    "lint-staged": "9.2.1",
    "node-sass": "4.12.0",
    "prettier": "1.18.2",
    "puppeteer": "1.19.0",
    "sass-loader": "7.1.0",
    "ts-jest": "24.0.2",
    "ts-node": "8.3.0",
    "typescript": "3.5.3",
    "vue-jest": "3.0.4",
    "vue-loader": "15.7.1",
    "vue-template-compiler": "2.6.10",
    "vue-tsx-support": "2.3.1",
    "webpack": "4.39.0",
    "yaml-loader": "0.5.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
