{
  "name": "vue-sample",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .ts,.js,.vue --format codeframe --color .",
    "lint:commit": ".scripts/lint_when_commit",
    "dev": "nuxt",
    "build": "nuxt build && tsc -p tsconfig.nuxt-config.json",
    "start": "./server.js start -c .nuxt/ts-compiled/nuxt.config.js"
  },
  "pre-commit": [
    "lint:commit"
  ],
  "dependencies": {
    "@nuxtjs/auth": "github:Yama-Tomo/auth-module#feature/allow_redirect_setting_by_function",
    "@nuxtjs/axios": "5.4.1",
    "@types/node": "10.14.3",
    "body-parser": "1.18.3",
    "lodash": "4.17.11",
    "nuxt": "2.5.1",
    "nuxt-i18n": "5.8.5",
    "nuxt-property-decorator": "2.1.3",
    "vuex-type-helper": "1.2.1"
  },
  "devDependencies": {
    "@nuxt/typescript": "2.5.1",
    "@nuxtjs/eslint-config": "0.0.1",
    "@types/body-parser": "1.17.0",
    "@types/hard-source-webpack-plugin": "1.0.0",
    "@types/lodash": "4.14.123",
    "@types/webpack": "4.4.26",
    "@typescript-eslint/eslint-plugin": "^1.5.0",
    "@vue/cli-plugin-babel": "3.5.1",
    "@vue/cli-plugin-typescript": "3.5.1",
    "@vue/cli-service": "3.5.1",
    "eslint": "^5.15.3",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "hard-source-webpack-plugin": "0.13.1",
    "json-loader": "0.5.7",
    "node-sass": "4.11.0",
    "pre-commit": "^1.2.2",
    "sass-loader": "7.1.0",
    "typescript": "3.3.4000",
    "yaml-loader": "0.5.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "eslintIgnore": [
    "node_modules",
    "__api",
    "dist",
    ".nuxt"
  ]
}
