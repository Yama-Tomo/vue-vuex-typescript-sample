{
  "name": "vue-sample",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development vue-cli-service serve --port 3100 src/index.ts",
    "build": "NODE_ENV=production vue-cli-service build --mode production",
    "build:dev": "NODE_ENV=development vue-cli-service build --mode development",
    "lint": "eslint --ext .ts,.tsx,.js,.vue --format codeframe --color",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn lint --fix"
    ]
  },
  "dependencies": {
    "vue": "2.6.12",
    "vuex": "3.6.2",
    "vuex-type-helper": "1.3.1"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config-typescript": "6.0.0",
    "@types/lodash": "4.14.168",
    "@typescript-eslint/eslint-plugin": "4.18.0",
    "@typescript-eslint/parser": "4.18.0",
    "@vue/cli-plugin-babel": "4.5.12",
    "@vue/cli-plugin-typescript": "4.5.12",
    "@vue/cli-service": "4.5.12",
    "eslint": "7.22.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-prettier": "3.3.1",
    "husky": "5.1.3",
    "javascript-typescript-langserver": "2.11.3",
    "lint-staged": "10.5.4",
    "node-sass": "5.0.0",
    "sass-loader": "10.1.1",
    "typescript": "4.2.3",
    "vue-template-compiler": "2.6.12",
    "vue-tsx-support": "3.1.0",
    "webpack-manifest-plugin": "3.1.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
